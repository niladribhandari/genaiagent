# GitHub Search Agent Configuration

# Agent Identification
AGENT_NAME=GitHubSearchAgent
AGENT_VERSION=1.0.0

# GitHub API Configuration
# GITHUB_TOKEN=your_github_personal_access_token_here
GITHUB_API_URL=https://api.github.com
GITHUB_API_VERSION=2022-11-28

# Search Configuration
MAX_SEARCH_RESULTS=50
DEFAULT_SORT=stars
DEFAULT_ORDER=desc
SEARCH_TIMEOUT=30

# Repository Filtering
MIN_STARS=1
MAX_RESULTS_PER_LANGUAGE=10
EXCLUDE_FORKS=false
INCLUDE_ARCHIVED=false

# Content Analysis
ENABLE_CODE_ANALYSIS=true
ENABLE_README_ANALYSIS=true
ENABLE_DOCUMENTATION_EXTRACTION=true
MAX_FILE_SIZE_KB=100

# Rate Limiting
REQUESTS_PER_MINUTE=30
ENABLE_RATE_LIMITING=true
BACKOFF_STRATEGY=exponential

# Caching Configuration
ENABLE_CACHING=true
CACHE_DURATION_HOURS=24
CACHE_DIRECTORY=./cache
MAX_CACHE_SIZE_MB=500

# Logging Configuration
LOG_LEVEL=info
LOG_FILE=./logs/github_search.log
ENABLE_API_LOGGING=false

# Search Filters
SUPPORTED_LANGUAGES=python,javascript,typescript,java,csharp,go,rust,php,ruby,swift,kotlin
EXCLUDE_PATTERNS=test,example,demo,tutorial,template
INCLUDE_PATTERNS=api,library,framework,tool

# Feature Flags
ENABLE_TRENDING_ANALYSIS=true
ENABLE_CONTRIBUTOR_ANALYSIS=false
ENABLE_ISSUE_ANALYSIS=false
ENABLE_SECURITY_SCANNING=true

# Integration Configuration
MCP_SERVER_URL=http://localhost:3001
WORKFLOW_AGENT_URL=http://localhost:8080

# Output Configuration
RESULT_FORMAT=json
INCLUDE_METADATA=true
INCLUDE_STATISTICS=true
ENABLE_RELEVANCE_SCORING=true
