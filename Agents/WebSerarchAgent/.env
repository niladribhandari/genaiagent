# Web Search Agent Configuration

# Agent Identification
AGENT_NAME=WebSearchAgent
AGENT_VERSION=1.0.0

# Search Engine Configuration
# SERP_API_KEY=your_serp_api_key_here
# GOOGLE_API_KEY=your_google_api_key_here
# GOOGLE_CSE_ID=your_custom_search_engine_id_here
DEFAULT_SEARCH_ENGINE=google
FALLBACK_SEARCH_ENGINE=bing

# Search Configuration
MAX_SEARCH_RESULTS=20
SEARCH_TIMEOUT=30
DEFAULT_LANGUAGE=en
DEFAULT_COUNTRY=us

# Content Processing
ENABLE_CONTENT_EXTRACTION=true
ENABLE_SUMMARIZATION=true
MAX_CONTENT_LENGTH=5000
ENABLE_LINK_VALIDATION=true

# Filtering Configuration
EXCLUDE_DOMAINS=spam.com,ads.com
INCLUDE_DOMAINS=stackoverflow.com,github.com,docs.python.org,developer.mozilla.org
CONTENT_QUALITY_THRESHOLD=0.7

# OpenAI Configuration (for summarization)
# OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-3.5-turbo
SUMMARIZATION_MAX_TOKENS=500

# Caching Configuration
ENABLE_CACHING=true
CACHE_DURATION_HOURS=12
CACHE_DIRECTORY=./cache
MAX_CACHE_SIZE_MB=200

# Rate Limiting
REQUESTS_PER_MINUTE=60
ENABLE_RATE_LIMITING=true
CONCURRENT_REQUESTS=5

# Logging Configuration
LOG_LEVEL=info
LOG_FILE=./logs/web_search.log
ENABLE_SEARCH_HISTORY=true

# Search Categories
ENABLE_DOCUMENTATION_SEARCH=true
ENABLE_TUTORIAL_SEARCH=true
ENABLE_EXAMPLE_SEARCH=true
ENABLE_NEWS_SEARCH=false

# Feature Flags
ENABLE_IMAGE_SEARCH=false
ENABLE_VIDEO_SEARCH=false
ENABLE_ACADEMIC_SEARCH=true
ENABLE_REAL_TIME_SEARCH=false

# Integration Configuration
MCP_SERVER_URL=http://localhost:3001
WORKFLOW_AGENT_URL=http://localhost:8080

# Output Configuration
RESULT_FORMAT=json
INCLUDE_SNIPPETS=true
INCLUDE_METADATA=true
ENABLE_RELEVANCE_SCORING=true

# Content Types
SUPPORTED_CONTENT_TYPES=text/html,application/pdf,text/plain
EXCLUDE_FILE_TYPES=.exe,.zip,.rar,.dmg
