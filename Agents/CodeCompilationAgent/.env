# Code Compilation Agent Configuration

# Agent Identification
AGENT_NAME=CodeCompilationAgent
AGENT_VERSION=1.0.0

# Compilation Configuration
SUPPORTED_LANGUAGES=python,javascript,typescript,java,csharp,go,rust
DEFAULT_TIMEOUT=300
MAX_CONCURRENT_COMPILATIONS=2

# Python Configuration
PYTHON_EXECUTABLE=python3
PYTHON_VENV_PATH=./venv
ENABLE_PYTHON_LINTING=true
PYTHON_LINTER=pylint

# JavaScript/TypeScript Configuration
NODE_EXECUTABLE=node
NPM_EXECUTABLE=npm
ENABLE_TS_COMPILATION=true
ENABLE_JS_LINTING=true

# Java Configuration
JAVA_HOME=/usr/lib/jvm/default-java
MAVEN_HOME=/usr/share/maven
GRADLE_HOME=/usr/share/gradle

# Build Configuration
BUILD_DIRECTORY=./build
OUTPUT_DIRECTORY=./dist
TEMP_DIRECTORY=./temp
CLEANUP_AFTER_BUILD=true

# Logging Configuration
LOG_LEVEL=info
LOG_FILE=./logs/compilation.log
ENABLE_BUILD_LOGS=true
KEEP_BUILD_LOGS_DAYS=7

# Error Handling
MAX_RETRY_ATTEMPTS=3
RETRY_DELAY=5
CONTINUE_ON_WARNINGS=true
FAIL_ON_LINTING_ERRORS=false

# Feature Flags
ENABLE_DEPENDENCY_INSTALLATION=true
ENABLE_SECURITY_SCANNING=false
ENABLE_PERFORMANCE_PROFILING=false
ENABLE_CODE_COVERAGE=false

# Integration Configuration
MCP_SERVER_URL=http://localhost:3001
WORKFLOW_AGENT_URL=http://localhost:8080
